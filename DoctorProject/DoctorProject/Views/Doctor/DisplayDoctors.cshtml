@model IEnumerable<DoctorProject.Models.DoctorModel>

@{
    ViewBag.Title = "DisplayDoctors";
}

<h2>DisplayDoctors</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DoctoreId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoctorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Salary)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DoctoreId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DoctorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Salary)
        </td>
       
    </tr>
}

</table>
