@model PresentationLayer.Models.StudentModel

@{
    ViewBag.Title = "getstudents";
}


<h2>Get student Names</h2>
<form action="displaystudents" method="post">
    <label for="id">Enter college id:</label>
    <input type="number" id="id" name="id" required />
    <br />
    <br />
    <br />
    <input type="submit" value="SEARCH" class="btn btn-default" />
</form>
<body style="background-color:aquamarine; font-family: Verdana, Arial;">
    <form action="" method="post"></form>
</body>


@if (TempData["status"] != null)
{
    <script type="text/javascript">
            window.onload = function () {
                alert("@TempData["status"]");
            };
    </script>
}



